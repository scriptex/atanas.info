@import 'itcss/main.css';
@import 'itcss/assets/tools/_chevron.css';
@import 'itcss/assets/tools/_flexbox.css';
@import 'itcss/assets/tools/_media-queries.css';
@import 'itcss/assets/tools/_mixins.css';
@import 'slick-carousel/slick/slick.css';
@import 'lite-youtube-embed/src/lite-yt-embed.css';
@import 'react-vertical-timeline-component/style.min.css';
@import 'react-round-carousel/dist/index.css';

@define-mixin slanted {
	color: var(--color-primary);
	display: inline-block;
	padding: 0.5rem 2rem 0.5rem 1rem;
	background: paint(slanted-background);
	--slanted-background-color: var(--color-action);
	--slanted-background-opacity: 0.35;

	/* @media (prefers-color-scheme: light) {
		color: var(--color-secondary);
	} */
}

@define-mixin polygon $sides {
	border-radius: 0;
	mask-image: paint(avatar-polygon);
	--avatar-sides: $sides;
	--avatar-angle: 0deg;
}

@define-mixin overlay {
	content: '';
	@mixin centered;
	z-index: 1;
}

/* variables */

:root {
	--font-sans-serif: system-ui;
	--color-base: #333;
	--color-primary: #fff;
	--color-secondary: #000;
	--color-action: #ef4c23;
	--color-calendar-graph-day-bg: #ebedf0;
	--color-calendar-graph-day-border: rgba(27, 31, 35, 0.06);
	--color-calendar-graph-day-L1-bg: #9be9a8;
	--color-calendar-graph-day-L2-bg: #40c463;
	--color-calendar-graph-day-L3-bg: #30a14e;
	--color-calendar-graph-day-L4-bg: #216e39;
}

@media (prefers-color-scheme: light) {
	:root {
		--color-base: #333;
		--color-primary: #fff;
		--color-secondary: #000;
		--color-action: #ef4c23;
	}
}

@keyframes float {
	0%,
	100% {
		transform: translate(0rem, 0rem);
	}

	50% {
		transform: translate(-1rem, -1rem);
	}
}

@keyframes stars {
	from {
		transform: translateY(0px);
	}

	to {
		transform: translateY(-2000px);
	}
}

@keyframes drop-vertical {
	from {
		top: -50%;
	}

	to {
		top: 110%;
	}
}

@keyframes drop-horizontal {
	from {
		left: -50%;
	}

	to {
		left: 110%;
	}
}

@keyframes loading {
	0% {
		width: 0;
		height: 0;
		opacity: 1;
	}

	100% {
		width: 4.5rem;
		height: 4.5rem;
		opacity: 0;
	}
}

/* global styles */

html,
body,
#root,
.o-main--high,
.c-section--hello {
	height: 100%;
}

body {
	color: var(--color-primary);
	background-color: var(--color-secondary);
}

@media (--hover) {
	a:hover {
		text-decoration: none;
	}
}

/* grid */

.o-grid__item {
	padding-bottom: 1rem;
}

@media (--mobile) {
	.o-grid__item + .o-grid__item {
		padding-top: 1rem;
	}
}

/* houdini backgrounds */

.bubbles,
.circles {
	background-attachment: initial;
}

.bubbles {
	background-image: paint(bubbles);
}

.circles {
	background-image: paint(circles);
}

/* svg icons */

[class^='svg-'] {
	display: inline-block;
	vertical-align: middle;
}

.c-svg-email {
	width: 1.5rem;
	height: 1.69rem;
	fill: currentColor;
}

.c-svg-github {
	width: 1.5rem;
	height: 1.63rem;
	fill: currentColor;
}

.c-svg-gitlab {
	width: 1.5rem;
	height: 1.63rem;
	fill: currentColor;
}

.c-svg-twitter {
	width: 1.5rem;
	height: 1.38rem;
	fill: currentColor;
}

.c-svg-npm {
	width: 1.5rem;
	height: 1.5rem;
	fill: var(--color-secondary);
	background-color: currentColor;
	border-radius: 0.25rem;
}

.c-svg-stackoverflow {
	width: 1.5rem;
	height: 1.5rem;
	fill: currentColor;
}

.c-svg-youtube {
	width: 3rem;
	height: 1.5rem;
	fill: currentColor;
}

.c-svg-linkedin {
	width: 1.5rem;
	height: 1.63rem;
	fill: currentColor;
}

.c-svg-logo {
	width: 3.13rem;
	height: 3.13rem;
	fill: currentColor;
	stroke: currentColor;
	stroke-width: 0.25rem;
}

.c-svg-google-plus {
	width: 1.5rem;
	height: 1.5rem;
	fill: currentColor;
}

.c-svg-codepen {
	width: 1.5rem;
	height: 1.5rem;
	fill: currentColor;
}

.c-svg-codersrank {
	width: 2.5rem;
	height: 1.5rem;
}

/* button */

.c-btn {
	font-size: 1rem;
	line-height: 1.25;
	color: var(--color-primary);
	font-weight: 400;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	letter-spacing: 0.04em;
	display: inline-block;
	vertical-align: middle;
	padding: 1rem 3rem;
	border: 1px solid var(--color-action);
	background-color: var(--color-action);
	border-radius: 0.25rem;
	box-shadow: none;
	appearance: none;
}

.c-btn--small {
	font-size: 0.875rem;
	line-height: 1;
	padding: 0.5rem 1rem;
}

@media (--hover) {
	.c-btn:hover {
		color: var(--color-action);
		background-color: var(--color-primary);
	}
}

/* header */

.c-header {
	@mixin flex-row nowrap, center, space-between;
	position: fixed;
	top: 0;
	right: 0;
	left: 0;
	z-index: 30;
	padding: 1rem;
	background-color: color-mod(var(--color-secondary) a(85%));

	& a {
		color: var(--color-primary);
		display: block;

		& svg {
			display: block;
		}
	}
}

/* nav */

.c-nav {
	font-size: 1.2rem;
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	right: 0;
	pointer-events: none;

	& ul,
	&::before {
		opacity: 0;
		visibility: hidden;
		transition: all var(--timing) var(--easing);
	}

	&::before {
		content: '';
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		z-index: 1;
		background-color: color-mod(var(--color-secondary) a(50%));
	}

	& ul {
		width: 20rem;
		height: 100%;
		display: flex;
		flex-flow: column wrap;
		position: relative;
		z-index: 2;
		margin: 0 0 0 auto;
		transform: translateX(100%);
	}

	& li,
	& a {
		width: 100%;
	}

	& li {
		flex: 1;
		opacity: 0;
		border-left: 1px solid;
		transform: translateX(100%);
	}

	& li + li {
		border-top: 1px solid;
	}

	& a {
		color: var(--color-primary);
		height: 100%;
		display: flex;
		flex-flow: row nowrap;
		align-items: center;
		justify-content: center;
		text-decoration: none;
		background-color: var(--color-secondary);

		&.active {
			color: var(--color-action);
			background-color: var(--color-primary);
		}
	}
}

@for $i from 1 to 20 {
	.c-nav li:nth-child($(i)) {
		/* prettier-ignore */
		transition-delay: calc(0.075s * $i);
	}
}

@media (--hover) {
	.c-nav a:hover {
		color: var(--color-action);
		background-color: var(--color-primary);
	}
}

.c-nav--inline {
	font-size: 0.875rem;
	position: relative;
	width: auto;
	height: auto;
	top: auto;
	right: auto;
	z-index: 24;
	padding-top: 2rem;
	pointer-events: auto;
	background-color: var(--color-secondary);

	&::before {
		content: none;
	}

	& ul {
		width: auto;
		height: auto;
		display: block;
		opacity: 1;
		visibility: visible;
		margin: 0;
		transform: none;
		transition: none;
		columns: 4;
		column-gap: 1rem;
	}

	& li {
		display: block;
		opacity: 1;
		padding-bottom: 0.5rem;
		border: 0;
		transform: none;
		transition: none;

		& + li {
			border: 0;
		}
	}

	& a {
		white-space: nowrap;
		width: auto;
		display: inline-flex;
		padding: 0;

		&.active {
			color: inherit;
			text-decoration: underline;
			text-decoration-color: var(--color-action);
			background-color: transparent;
		}
	}
}

@media (--hover) {
	.c-nav--inline a:hover {
		color: inherit;
		text-decoration: underline;
		text-decoration-color: var(--color-action);
		background-color: transparent;
	}
}

@media (--mobile) {
	.c-nav--inline ul {
		column-count: 2;
		column-gap: 0;
	}
}

/* Nav toggle */

.c-nav__toggle {
	color: var(--color-primary);
	width: 3.13rem;
	height: 3.13rem;
	display: flex;
	flex-flow: column wrap;
	align-items: center;
	justify-content: center;
	position: relative;
	z-index: 2;
	padding: 0;
	border: 0;
	background: none;
	cursor: pointer;
	transform: rotate(0deg);
	transition: transform var(--timing) var(--easing);
	appearance: none;

	&:focus {
		color: var(--color-secondary);
		outline: 0 none;
		background-color: var(--color-primary);
	}

	& span {
		width: 1.25rem;
		height: 0.1875rem;
		display: block;
		opacity: 1;
		position: relative;
		top: 0;
		margin: 0.095rem auto;
		background-color: currentColor;
		transform: rotate(0deg);
		transition: all var(--timing) var(--easing);
	}
}

/* Nav open state */

.c-header--open {
	& .c-nav {
		pointer-events: all;

		& ul,
		&::before {
			opacity: 1;
			visibility: visible;
		}
	}

	& ul {
		transform: translateX(0);
	}

	& li {
		opacity: 1;
		transform: translateX(0%);
		transition: all var(--timing) var(--easing);
	}

	& .c-nav__toggle {
		position: absolute;
		top: 1rem;
		right: 1.125rem;
		transform: rotate(90deg) translate(-1rem, -1.125rem);

		& span:nth-child(2) {
			opacity: 0;
		}

		& span:first-child {
			top: 0.375rem;
			transform: rotate(45deg);
		}

		& span:last-child {
			top: -0.375rem;
			transform: rotate(-45deg);
		}
	}
}

/* sections */

.c-section {
	overflow: hidden;
}

.c-section__video {
	width: auto;
	height: auto;
	min-width: 100%;
	min-height: 100%;
	max-width: none;
	max-height: none;
	opacity: 0.2;
	position: fixed;
	top: 50%;
	left: 50%;
	z-index: -1;
	transform: translate(-50%, -50%);
	pointer-events: none;
}

.c-section__actions {
	text-align: center;
	position: relative;
	z-index: 2;
	padding-top: 1.5rem;

	& .c-btn {
		margin: 0 1rem;
	}
}

.c-section__actions-list {
	@mixin flex-row wrap, center, center;
	position: absolute;
	top: -2rem;
	left: 50%;
	padding: 0;
	margin: 0;
	list-style: none outside none;
	transform: translateX(-50%);
	pointer-events: none;

	&::before {
		content: '';
		opacity: 0;
		visibility: hidden;
		position: absolute;
		top: -10rem;
		right: -5rem;
		bottom: -10rem;
		left: -5rem;
		z-index: 1;
		background-color: color-mod(var(--color-secondary) a(80%));
		border-radius: 50%;
		transform: scale(0);
		transition: all var(--timing) var(--easing);
	}

	& li {
		z-index: 2;
		opacity: 0;
		visibility: hidden;
		transform: scale(0) translate(0);
		transition: all var(--timing) var(--easing);

		&:first-child {
			position: absolute;
			bottom: 18%;
			left: 50%;
			transform: translateX(-50%);
		}

		&:not(:first-child) {
			& .c-btn {
				font-size: 1rem;
				line-height: 1;
				text-transform: none;
				width: 10rem;
				height: 10rem;
				@mixin flex-row wrap, center, center;
				align-content: center;
				padding: 1.5rem;
				border: 1px solid var(--color-primary);
				background: none;
				border-radius: 50%;
				transition: all var(--timing) var(--easing);
			}
		}
	}

	& .c-btn {
		margin: 0;

		& span,
		& strong {
			flex: 0 0 100%;
			display: block;
		}

		& span {
			padding: 0.5rem 0;
		}

		& svg {
			width: 3rem;
			height: 3rem;
			fill: currentColor;
			display: block;
			margin: auto;
			transition: none;
		}
	}
}

.c-section__actions-list--open {
	pointer-events: auto;

	& li,
	&::before {
		opacity: 1;
		visibility: visible;
		transform: scale(1);
	}

	& li {
		&:nth-child(2) {
			transform: scale(1) translate(-2rem, -1rem);
		}

		&:nth-child(3) {
			transform: scale(1) translate(0, -8rem);
		}

		&:nth-child(4) {
			transform: scale(1) translate(2rem, -1rem);
		}
	}
}

.c-section__actions-close {
	color: var(--color-primary);
	font-size: 0;
	line-height: 0;
	width: 2rem;
	height: 2rem;
	display: block;
	padding: 0;
	border: 1px solid var(--color-action);
	background-color: var(--color-action);
	border-radius: 50%;
	box-shadow: none;

	&::before,
	&::after {
		content: '';
		width: 0.125rem;
		height: 1.25rem;
		display: block;
		@mixin centered;
		z-index: 2;
		background-color: currentColor;
	}

	&::before {
		transform: rotate(45deg);
	}

	&::after {
		transform: rotate(-45deg);
	}
}

@media (--hover) {
	.c-section__actions-list {
		& li {
			&:not(:first-child) {
				& .c-btn:hover {
					color: var(--color-action);
					border-color: var(--color-action);
					background-color: var(--color-primary);
				}
			}
		}
	}
}

@media (--tablet-portrait) {
	.c-section__actions-list {
		width: 70vw;
	}
}

@media (--mobile) {
	.c-section__actions {
		& .c-btn {
			margin: 0;

			& ~ .c-btn {
				margin-top: 2rem;
			}
		}
	}

	.c-section__actions-list {
		width: 90vw;
		top: auto;
		bottom: -1rem;

		&::before {
			top: -8rem;
			right: 0;
			bottom: -8rem;
			left: 0;
		}

		& li {
			&:first-child {
				bottom: 1.75rem;
			}

			&:not(:first-child) {
				& .c-btn {
					font-size: 0.75rem;
					line-height: 1;
					width: 6rem;
					height: 6rem;
					padding: 0.25rem;
				}
			}
		}

		& .c-btn {
			& svg {
				width: 1rem;
				height: 1rem;
			}

			& span {
				padding: 0.25rem 0;
			}
		}
	}

	.c-section__actions-list--open {
		& li {
			&:nth-child(2) {
				transform: scale(1) translate(-0, -1rem);
			}

			&:nth-child(3) {
				transform: scale(1) translate(0, -6rem);
			}

			&:nth-child(4) {
				transform: scale(1) translate(0, -1rem);
			}
		}
	}
}

/* Section Hello */

.c-section--hello {
	text-align: center;
	position: relative;
	z-index: 1;
	overflow: hidden;

	& .o-shell {
		height: 100%;
	}

	& h1 {
		margin: 0;
	}

	& h2 {
		min-height: 6.25rem;
		position: relative;
		overflow: hidden;
	}
}

.c-canvas {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 1;
	background-color: var(--color-secondary);

	& svg {
		width: 100%;
		height: 100%;
		display: block;
	}
}

.c-slider {
	width: 30rem;
	position: absolute;
	top: 50%;
	left: 50%;
	z-index: 2;
	transform: translate(-50%, -50%);

	& h2 {
		width: 100%;
		perspective: 50rem;
		perspective-origin: 50% 10rem;
	}

	& span {
		width: 100%;
		text-align: center;
		position: absolute;
		top: 0;
		right: 0;
		left: 0;
		opacity: 0;
		transition: all var(--timing) var(--easing);
		transform: rotateY(-90deg) translateZ(9rem);

		&:nth-child(odd) {
			transform: rotateY(90deg) translateZ(9rem);
		}

		&.current {
			opacity: 1;
			transform: rotateY(0deg) translateZ(0);
		}
	}
}

@media (--mobile) {
	.c-slider {
		width: 18rem;

		& h1 {
			font-size: 1.75rem;
		}

		& h2 {
			font-size: 1.5rem;
		}
	}
}

/* Section About */

.c-section--about {
	padding: 8rem 0 4rem;
	position: relative;
	z-index: 2;
	overflow: visible;
	background-attachment: fixed;

	&::before {
		@mixin overlay;
		opacity: 0.5;
		background-color: var(--color-secondary);
	}

	& .o-shell {
		position: relative;
		z-index: 2;
	}
}

.c-article {
	& header {
		text-align: center;
		flex: 0 0 100%;
	}

	& h2 {
		text-align: center;
		margin-bottom: 4rem;
		@mixin slanted;
	}

	& h3 {
		@mixin slanted;
	}

	& img {
		flex: 1;
		max-width: 15rem;
		display: block;
		position: sticky;
		top: 7rem;
		left: 0;
		margin-top: -2rem;
		border-radius: 50%;
	}

	& .o-shell {
		@mixin flex-row wrap, flex-start, flex-start;
	}

	& .c-section__actions {
		text-align: center;
		flex: 0 0 100%;
		padding-top: 3.5rem;
	}
}

.c-article__block {
	flex: 0 0 35%;
	position: relative;
	margin-bottom: 1rem;

	&:nth-of-type(1) {
		flex: 0 0 55%;
		margin-left: 3rem;
	}

	&:nth-of-type(2) {
		margin: 8rem 3rem 1rem 18rem;
	}

	&:nth-of-type(3) {
		margin-top: 3rem;
	}

	&:nth-of-type(4) {
		margin: 4rem 3rem 1rem 18rem;
	}

	&:nth-of-type(5) {
		margin-top: 1rem;
	}

	&:nth-of-type(6) {
		flex: 0 0 60%;
		margin-top: 4rem;
		margin-left: 18rem;
	}
}

@supports (background: paint(avatar-polygon)) {
	.c-article img {
		@mixin polygon 12;
	}
}

@media (--tablet-landscape) {
	.c-article__block {
		flex: 0 0 30%;
	}
}

@media (--tablet-portrait) {
	.c-article {
		& img {
			position: static;
			top: auto;
			left: auto;
			margin: 0 auto 2rem;
		}

		& .c-article__block,
		& .c-article__block:nth-of-type(1),
		& .c-article__block:nth-of-type(2),
		& .c-article__block:nth-of-type(3),
		& .c-article__block:nth-of-type(4),
		& .c-article__block:nth-of-type(5),
		& .c-article__block:nth-of-type(6) {
			flex: 0 0 100%;
			margin: 0 0 2rem;
		}

		& .c-article__block:nth-of-type(6) {
			margin-bottom: 0;
		}
	}
}

@media (--mobile) {
	.c-article {
		display: block;
		padding: 7rem 0 2rem;

		& h2 {
			margin-bottom: 2rem;
		}

		& h2,
		& h3 {
			text-align: center;
		}

		& .c-article__block:nth-of-type(6) {
			margin-bottom: 2rem;
		}

		& .c-section__actions {
			padding-top: 0;
		}
	}
}

/* Section Skills */

.c-section--skills {
	color: var(--color-primary);
	text-align: center;
	position: relative;
	z-index: 2;
	padding: 8rem 0 4rem;

	&::before {
		@mixin overlay;
		z-index: 1;
		opacity: 0.8;
		background-color: var(--color-secondary);
	}

	& h2 {
		font-size: 4vw;
		margin-bottom: 2rem;

		& small {
			font-size: 1rem;
			@mixin slanted;
		}
	}

	& svg {
		width: 100%;
		display: block;
		overflow: visible;
	}

	& text {
		font-size: 1rem;
		line-height: 1;
		fill: var(--color-primary);
		text-shadow: 0 0 2rem var(--color-secondary);
		opacity: 0;
		visibility: hidden;
		transition: opacity var(--timing) var(--easing), visibility var(--timing) var(--easing);
		pointer-events: none;
		user-select: none;
	}

	& line {
		stroke: var(--color-primary);
		stroke-width: 0.1275rem;
	}

	& use {
		opacity: 1;
		visibility: visible;
		transition: opacity var(--timing) var(--easing), visibility var(--timing) var(--easing);
		pointer-events: none;
	}

	& circle {
		stroke: var(--color-primary);
		stroke-width: 0.1275rem;
		cursor: all-scroll;
		transition: fill var(--timing) var(--easing), stroke var(--timing) var(--easing),
			stroke-width var(--timing) var(--easing);

		&:hover {
			fill: var(--color-action);
			stroke: var(--color-action);
			stroke-width: 1rem;

			& ~ text {
				opacity: 1;
				visibility: visible;
			}

			& ~ use {
				opacity: 0;
				visibility: hidden;
			}
		}
	}

	& .o-grid {
		max-height: 97.5rem;
		justify-content: flex-start;
		overflow: hidden;
		transition: max-height var(--timing) var(--easing);

		&.is--hidden {
			max-height: 0;
		}
	}

	& .o-shell {
		position: relative;
		z-index: 2;
	}
}

#skills-graph {
	height: 100vh;
	position: relative;
	transition: height var(--timing) var(--easing);

	& svg {
		opacity: 1;
		position: absolute;
		top: 50%;
		left: 50%;
		transform: translate(-50%, -50%);
		transition: opacity var(--timing) var(--easing);
	}

	&.is--hidden {
		height: 0vh;
		pointer-events: none;

		& svg {
			opacity: 0;
		}
	}
}

.c-skill {
	color: var(--color-secondary);
