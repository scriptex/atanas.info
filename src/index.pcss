/* Section Portfolio */

.c-section--portfolio {
	color: var(--color-primary);
	text-align: center;
	position: relative;
	z-index: 2;
	padding: 8.25rem 0 3.25rem;
	margin-bottom: 4rem;

	& .o-shell {
		position: relative;
		z-index: 2;
	}

	& h1,
	& h2 {
		text-align: center;
		flex: 0 0 100%;
		margin-bottom: 3.5rem;
	}

	& h2 {
		@mixin slanted;
	}

	& .c-section__body {
		@mixin flex-row wrap, stretch, space-between;

		& a {
			font-size: 1.25rem;
			line-height: 1.25;
			color: var(--color-primary);
			text-decoration: none;
			width: calc(50% - 1rem);
			height: 1px;
			display: block;
			flex: 0 0 calc(50% - 1rem);
			position: relative;
			padding-bottom: 36%;
			margin-bottom: 2rem;
			overflow: hidden;

			& img,
			& object {
				position: absolute;
				top: 50%;
				left: 50%;
				z-index: 1;
				transform: translate(-50%, -50%);
				transition: all var(--timing) var(--easing);
			}

			&.disabled {
				cursor: default;
			}

			&::before {
				content: '';
				@mixin centered;
				opacity: 0;
				z-index: 2;
				background-color: color-mod(var(--color-secondary) a(80%));
				transition: opacity var(--timing) var(--easing);
			}

			& div {
				display: block;
				opacity: 0;
				position: absolute;
				top: 50%;
				right: 0;
				left: 0;
				z-index: 3;
				padding: 1rem;
				transform: translateY(0%);
				transition: all var(--timing) var(--easing);
			}

			& strong {
				text-transform: uppercase;
				display: block;
			}

			& span {
				font-family: monospace;
				font-size: 0.875rem;
				text-transform: lowercase;
			}

			& small {
				font-size: 100%;
				font-family: var(--font-sans-serif);
			}
		}
	}
}

.c-section__sliders {
	@mixin flex-row wrap, center, center;
}

.c-section__slider {
	width: calc(50% - 1rem);
	flex: 0 0 calc(50% - 1rem);
	margin: 0 0.5rem 3.5rem;
	overflow: hidden;

	& img {
		display: block;
		border: 1px solid var(--color-primary);
	}

	& div[style] {
		vertical-align: top;
	}

	& a {
		color: inherit;
		text-decoration: none;
		display: block;
		padding: 1rem 1rem 0.5rem;
		border: 1px solid var(--color-primary);
		background-color: var(--color-secondary);

		& h3,
		& p,
		& code {
			display: block;
			margin: 0 0 0.5rem;
		}
	}

	& .slick-arrow {
		font-size: 0;
		line-height: 0;
		text-indent: -100%;
		width: 2.5rem;
		height: 2.5rem;
		display: block;
		position: absolute;
		top: 50%;
		z-index: 2;
		border: 1px solid;
		margin-top: -1.25rem;
		background: var(--color-action);
		border-radius: 50%;
		box-shadow: none;

		&::before,
		&::after {
			content: '';
			@mixin centered;
		}

		&::before {
			width: 50%;
			height: 1px;
			background-color: var(--color-primary);
		}

		&::after {
			width: 50%;
			height: 1px;
			@mixin chevron 0.9375rem, 0 0 1px 1px, var(--color-primary), auto, -135deg;
		}
	}

	& .slick-prev {
		left: 0.5rem;
	}

	& .slick-prev::after {
		transform: rotate(45deg);
	}

	& .slick-next {
		right: 0.5rem;
	}
}

@media (--hover) {
	.c-section--portfolio {
		& .c-section__body {
			& a {
				&:hover {
					box-shadow: 0 0 4.75rem color-mod(var(--color-primary) a(40%));
					background-size: 110% auto;
				}

				&:hover::before {
					opacity: 1;
				}

				&:hover div {
					opacity: 1;
					transform: translateY(-50%);
				}

				&:hover img,
				&:hover object {
					transform: translate(-50%, -50%) scale(1.1);
				}
			}
		}
	}
}

@media (--tablet-portrait) {
	.c-section__sliders {
		display: block;
	}

	.c-section__slider {
		max-width: 37.5rem;
		width: 100%;
		margin: 0 auto 3.5rem;
	}
}

@media (--mobile) {
	.c-section--portfolio {
		padding: 7rem 0 2rem;
		margin-bottom: 2rem;

		& h1,
		& h2,
		& .c-section__slider {
			margin-bottom: 2rem;
		}

		& object {
			width: 100%;
		}

		& .c-section__body {
			display: block;
			padding-bottom: 1rem;

			& a {
				font-size: 1rem;
				width: 100%;
				padding-bottom: calc(75% + 92px);
				margin-bottom: 1rem;
				background-position: 50% 92px;

				&::before {
					content: none;
				}

				& div,
				& img,
				& object {
					position: static;
					top: auto;
					right: auto;
					left: auto;
					transform: none !important;
				}

				& div {
					min-height: 92px;
					@mixin flex-row wrap, center, center;
					align-content: center;
					opacity: 1;
					padding: 0.5rem;
					background-color: var(--color-secondary);
				}

				& strong {
					flex: 0 0 100%;
				}

				&:hover {
					box-shadow: none;
					background-size: 100% auto;
				}

				&:hover div {
					transform: none;
				}
			}
		}

		& .c-section__actions {
			padding-top: 0;
		}
	}
}

/* Section Slides */

.c-section--slides,
.c-section--videos {
	color: var(--color-secondary);
	text-align: center;
	position: relative;
	z-index: 2;
	padding: 9.25rem 0 4rem;
	background-attachment: fixed;

	& .o-shell {
		position: relative;
		z-index: 2;
	}

	& + .c-section--slides,
	& + .c-section--videos {
		margin-top: 4rem;
	}

	& h2 {
		text-align: center;
		margin-bottom: 4rem;
		@mixin slanted;
	}

	/* prettier-ignore */
	& iframe,
	& lite-youtube { /* stylelint-disable-line selector-type-no-unknown */
		max-width: none;
		width: 100%;
		height: 100%;
		display: block;
		position: absolute;
		top: 0;
		left: 0;
		border: 0;
	}

	& .c-section__nav {
		padding-bottom: 2rem;
	}

	& .c-section__body {
		position: relative;
		padding-bottom: 3rem;

		&.o-grid {
			padding-bottom: 1.5rem;
		}
	}

	& .c-section__actions {
		text-align: center;
	}

	& .c-section__frame {
		height: 0;
		opacity: 0;
		visibility: hidden;
		position: absolute;
		top: 0;
		right: 0;
		left: 0;
		padding-bottom: 62.5%;
		transition: all var(--timing) var(--easing);

		&.current {
			position: relative;
			opacity: 1;
			visibility: visible;
		}
	}
}

.c-section--slides {
	&::before {
		@mixin overlay;
		opacity: 0.5;
		z-index: 1;
		background-color: var(--color-secondary);
	}
}

.c-section--videos {
	background-color: var(--color-secondary);
}

/* section articles */

.c-section--articles {
	&::before {
		opacity: 0.5;
		background-color: var(--color-primary);
	}
}

.c-article-link {
	color: var(--color-primary);
	text-align: center;
	text-decoration: none;
	height: 0;
	display: block;
	position: relative;
	padding-bottom: 65%;
	overflow: hidden;
	background-size: auto 100%;
	transition: background-size var(--timing) var(--easing);

	& strong {
		font-size: 1.25rem;
		opacity: 1;
		position: absolute;
		top: 50%;
		right: 0;
		left: 0;
		z-index: 2;
		transform: translateY(-50%);
		transition: all var(--timing) var(--easing);
	}

	&::before {
		@mixin overlay;
		opacity: 0.5;
		background-color: var(--color-secondary);
		transition: opacity var(--timing) var(--easing);
	}
}

@media (--hover) {
	.c-article-link {
		&:hover {
			background-size: auto 110%;

			&::before {
				opacity: 0;
			}

			& strong {
				opacity: 0;
				transform: translateY(100%);
			}
		}
	}
}

@media (--mobile) {
	.c-section--slides {
		padding: 7rem 0 2rem;

		& h2 {
			margin-bottom: 2rem;
		}

		& .c-section__nav {
			& .c-btn {
				width: 100%;
			}
		}

		& .c-section__body {
			padding-bottom: 0;

			&.o-grid {
				padding-bottom: 0;
				margin-bottom: -0.25rem;
			}
		}

		& .c-section__actions {
			padding-top: 2rem;
		}
	}

	.c-section--articles {
		& .o-grid__item + .o-grid__item {
			padding-top: 0;
		}

		& .c-section__actions {
			padding-top: 1rem;
		}
	}
}

/* section certificates */

.c-section--certificates {
	color: var(--color-primary);
	text-align: center;
	position: relative;
	z-index: 2;
	padding: 8rem 0 5rem;

	& h1 {
		text-align: center;
		margin-bottom: 4rem;
		@mixin slanted;
	}

	& .c-section__body {
		padding-bottom: 2.5rem;
	}
}

.c-certificate {
	display: block;

	& img {
		width: 100%;
		height: auto;
		display: block;
	}
}

@media (--mobile) {
	.c-section--certificates {
		padding: 7rem 0 2rem;

		& h1 {
			margin-bottom: 2rem;
		}

		& .c-section__body {
			padding-bottom: 0;
		}

		& .c-section__actions {
			padding-top: 1rem;
		}

		& .o-grid__item + .o-grid__item {
			padding-top: 0;
		}
	}
}

/* section timeline */

.c-section--timeline {
	text-align: center;
	position: relative;
	z-index: 2;
	padding: 8rem 0 4rem;
	overflow: hidden;

	& h2 {
		margin-bottom: 4rem;
		@mixin slanted;
	}

	& .vertical-timeline {
		color: var(--color-secondary);
		text-align: left;
		margin-bottom: 2.5rem;

		&::before {
			background-color: var(--color-action);
		}
	}

	& .vertical-timeline-element-icon {
		background-color: var(--color-primary);
		box-shadow: 0 0 0 0.25rem var(--color-action), inset 0 0.125rem 0 rgba(0, 0, 0, 0.08),
			0 0.125rem 0 0.25rem rgba(0, 0, 0, 0.05);
	}

	& .vertical-timeline-element-subtitle {
		padding-bottom: 1.25rem;
		border-bottom: 0.25rem solid var(--color-action);
	}

	& .vertical-timeline-element-content {
		padding: 1rem;
		box-shadow: 0 0 0 0.25rem var(--color-action);
	}

	& .vertical-timeline-element-content-arrow {
		top: 16px !important;
		border-width: 1rem;
		border-right-color: var(--color-action);
	}

	& .vertical-timeline-element-date {
		text-align: left !important;
		position: static !important;
		top: auto !important;
		right: auto !important;
		left: auto !important;
	}
}

@media (--tablet-landscape) {
	.c-section--timeline {
		& .vertical-timeline-element-content-arrow {
			top: 0.625rem !important;
			border-width: 0.75rem;
		}
	}
}

@media (--mobile) {
	.c-section--timeline {
		padding: 7rem 0 2rem;
		border-bottom-width: 4rem;

		& h2 {
			margin-bottom: 2rem;
		}

		& .vertical-timeline {
			margin-bottom: 0.5rem;
		}
	}
}

/* footer */

.c-footer {
	position: relative;
	z-index: 25;
	padding: 2rem 0;
	background-color: var(--color-secondary);

	& p {
		margin: 0;
	}

	& small {
		display: block;
	}

	& iframe {
		display: block;
		margin: 0.5rem auto 0;
	}

	& .o-shell {
		align-items: center;

		& + .o-shell {
			padding-top: 1rem;
		}
	}

	& .c-btn {
		margin-top: 1.5rem;
	}
}

@media (--tablet-portrait) {
	.c-footer {
		text-align: center;

		& p {
			flex: 0 0 100%;
			margin-bottom: 1rem;
		}
	}
}

/* prettier-ignore */
social-links { /* stylelint-disable-line selector-type-no-unknown */
	display: block;
}

/* prettier-ignore */
@media (--tablet-portrait) {
	social-links { /* stylelint-disable-line selector-type-no-unknown */
		flex: 0 0 100%;
	}
}

/* contact */

.c-contact {
	color: var(--color-secondary);
	@mixin flex-row wrap, center, center;
	opacity: 0;
	visibility: hidden;
	@mixin centered;
	position: fixed;
	z-index: 40;
	pointer-events: none;

	&::before {
		content: '';
		opacity: 0;
		visibility: hidden;
		@mixin centered;
		z-index: 1;
		background-color: var(--color-secondary);
		border-radius: 50%;
		transform: scale(0);
		transition: all var(--timing) var(--easing) 0.25s;
	}
}

.c-contact__close {
	color: var(--color-primary);
	font-size: 0;
	line-height: 0;
	width: 2rem;
	height: 2rem;
	position: absolute;
	top: 1rem;
	right: 1rem;
	z-index: 3;
	padding: 0;
	border: 1px solid var(--color-action);
	background-color: var(--color-action);
	border-radius: 50%;
	box-shadow: none;
	transform: rotate(-180deg) scale(0);
	transition: all var(--timing) var(--easing) 0.5s;

	&::before,
	&::after {
		content: '';
		width: 0.125rem;
		height: 1.25rem;
		display: block;
		@mixin centered;
		z-index: 2;
		background-color: currentColor;
	}

	&::before {
		transform: rotate(45deg);
	}

	&::after {
		transform: rotate(-45deg);
	}
}

@media (--hover) {
	.c-contact__close:hover {
		color: var(--color-action);
		background-color: var(--color-primary);
	}
}

.c-contact__body {
	width: 25rem;
	margin: auto;
	position: relative;
	z-index: 2;
	transform: translateY(-100vh);
	transition: all var(--timing) var(--easing) 0.75s;

	& p,
	& h2,
	& h4 {
		color: var(--color-primary);
		text-align: center;
		margin: 0 0 1.5rem;
	}
}

.c-contact__field {
	padding-bottom: 1rem;

	& label {
		color: var(--color-primary);
		display: inline-block;
		vertical-align: top;
		margin-bottom: 0.5rem;
	}

	& input,
	& textarea {
		width: 100%;
		display: block;
		padding: 0.5rem;
		border: 1px solid var(--color-action);
		background-color: var(--color-primary);
		border-radius: 0.25rem;
		box-shadow: none;
		appearance: none;
		resize: none;
	}
}

.c-contact__error,
.c-contact__success {
	color: var(--color-primary);
	text-align: center;
}

.c-contact__actions {
	& .c-btn {
		width: 100%;
		display: block;
	}
}

.c-contact--visible {
	opacity: 1;
	visibility: visible;
	pointer-events: auto;

	&::before {
		opacity: 0.95;
		visibility: visible;
		border-radius: 0;
		transform: scale(1);
	}

	& .c-contact__close {
		transform: rotate(0deg) scale(1);
	}

	& .c-contact__body {
		transform: translateX(0);
	}
}

@media (--mobile) {
	.c-contact {
		display: block;
		padding: 3.5rem 1rem 1rem;
	}

	.c-contact__body {
		width: auto;
	}
}

/* Loading */

.c-loader {
	width: 5rem;
	height: 5rem;
	@mixin centered;

	&::before,
	&::after {
		content: '';
		display: block;
		opacity: 1;
		@mixin centered;
		border: 0.125rem solid var(--color-primary);
		border-radius: 50%;
		animation: loading 2s cubic-bezier(0, 0.2, 0.8, 1) infinite;
	}

	&::after {
		border-color: var(--color-action);
		animation-delay: -1s;
	}
}

.c-loader__wrapper {
	height: 100%;
	position: relative;
	background-color: var(--color-secondary);
}

.carousel__control {
	background: var(--color-action);
}

@media (--mobile) {
	.carousel {
		margin: -0.25rem auto 1.5rem;
	}
}

.c-skyline__nav {
	& ul {
		padding: 0;
	}

	& .current .c-btn {
		color: var(--color-action);
		background-color: var(--color-primary);
		cursor: default;
	}
}

.c-skyline__item {
	position: relative;

	& canvas {
		width: 100%;
		height: 60vh;
		display: block;
	}

	& .c-loading {
		@mixin centered;
	}
}

.c-error-page {
	height: 100%;
	position: relative;

	& .o-shell--flex {
		height: 100%;
		justify-content: center;
		align-items: center;
	}

	& svg {
		display: block;
		overflow: visible;
	}

	& path {
		animation: float 10s infinite ease-in-out alternate;

		&:nth-child(2) {
			animation-delay: 1s;
		}
		&:nth-child(3) {
			animation-delay: 2s;
		}
		&:nth-child(4) {
			animation-delay: 3s;
		}
		&:nth-child(5) {
			animation-delay: 4s;
		}
	}
}

.c-error-page__content {
	font-size: 1.25rem;
	flex: 0 0 18.75rem;
	position: relative;
	z-index: 2;
	padding: 1rem;

	& h1 {
		font-size: 3rem;
		@mixin slanted;
	}

	& p,
	& h1 {
		margin-bottom: 1.25rem;
	}
}

@media (--mobile) {
	.c-error-page {
		& svg {
			opacity: 0.5;
			position: absolute;
			top: 0;
			right: 0;
			left: 0;
			bottom: 0;
			z-index: 1;
			margin: auto;
		}
	}
}

.c-funding {
	& svg {
		max-width: 37.5rem;
		opacity: 0;
		display: block;
		position: fixed;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		z-index: 100;
		margin: auto;
		overflow: visible;
		pointer-events: none;
		transform: scale(0);
		transition: all var(--timing) var(--easing);
	}

	& path {
		transition: fill var(--timing) var(--easing);
	}
}

.c-funding__item {
	color: inherit;
	text-decoration: none;

	& a {
		text-decoration: none;
		touch-action: auto !important;
	}

	& path {
		fill: var(--color-primary);
	}

	&:hover {
		& path {
			fill: var(--color-action);
		}
	}
}

.c-funding__trigger {
	fill: var(--color-action);
	pointer-events: auto;
	cursor: pointer;
}

.c-funding__backdrop {
	opacity: 0;
	display: block;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 90;
	background-color: var(--color-secondary);
	transition: opacity var(--timing) var(--easing);
	pointer-events: none;
}

.c-funding--open {
	& svg {
		opacity: 1;
		transform: scale(1);
		pointer-events: auto;
	}

	& .c-funding__backdrop {
		opacity: 0.8;
		pointer-events: auto;
	}
}

@media (prefers-reduced-motion: reduce) {
	* {
		animation-duration: 0.01ms !important;
		animation-iteration-count: 1 !important;
		transition-duration: 0.01ms !important;
		scroll-behavior: auto !important;
	}
}

