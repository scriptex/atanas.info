	"scripts": {
		"pwa": "npx create-pwa --icon public/images/icons/icon.png --launch public/images/icons/splash.png",
		"svg": "./bin/svg.sh",
		"time": "./bin/time.sh",
		"mjml": "mjml -w ./email/*.mjml -o ./email/",
		"insights": "ts-node ./insights/index.ts",
		"npm-stats": "tsx ./npm/index.ts ",
		"screenshots": "ts-node ./screenshots/index.ts",
		"fetch-music": "ts-node ./bin/fetch-music.ts",
		"fetch-projects": "ts-node ./bin/fetch-projects.ts",
		"tsc": "tsc --noEmit",
		"houdini": "tsc -p ./tsconfig-houdini.json",
		"next:dev": "open http://localhost:1234 && next dev -p 1234 --webpack",
		"next:start": "next start --webpack",
		"next:build": "next build --webpack",
		"start": "yarn time && yarn houdini && yarn next:dev",
		"start-dist": "http-server ./dist -p=1234 -P=http://localhost:1234/? -o",
		"lint": "./bin/lint.sh",
		"test": "NODE_ENV=test jest --config=./test-config/jest.config.ts --rootDir=./ --env=jsdom --detectOpenHandles",
		"prettier": "./bin/prettier.sh",
		"test:watch": "yarn test --watch",
		"test:coverage": "yarn test --coverage",
		"type-coverage": "type-coverage --ignore-catch --detail",
		"test:coverage:watch": "yarn test --coverage --watchAll",
		"pre": "yarn time && yarn fetch-projects && yarn svg",
		"post": "yarn tsc && yarn houdini && yarn next:build && next-sitemap",
		"build": "yarn pre && yarn post",
		"prepare": "husky"
	},
