{
	"name": "atanas.info",
	"version": "1.1.0",
	"description": "Atanas Atanasov's Personal Website",
	"scripts": {
		"fetch-stats": "ts-node ./src/scripts/fetch-stats.ts",
		"fetch-music": "ts-node ./src/scripts/fetch-music.ts",
		"fetch-projects": "ts-node ./projects/.vuepress/fetch-projects.ts",
		"start-projects": "cd projects && vuepress dev",
		"build-projects": "cd projects && vuepress build",
		"localga": "ts-node ./src/scripts/local-analytics.ts",
		"svg": "./bin/svg.sh",
		"tsc": "tsc --noEmit --skipLibCheck",
		"tsc:watch": "yarn tsc --watch",
		"houdini": "tsc -p tsconfig-houdini.json",
		"houdini:watch": "yarn houdini --watch",
		"time": "./bin/time.sh",
		"css": "postcss ./src/index.pcss -o ./src/index.css --config config",
		"css:watch": "postcss ./src/index.pcss -o ./src/index.css -w --config config",
		"parcel-start": "parcel ./src/index.html --open",
		"parcel-build-src": "rm -rf dist && parcel build ./src/index.html --public-url '/' --no-source-maps",
		"parcel-start-music": "parcel ./music/index.html --open --out-dir='dist/music'",
		"parcel-build-music": "parcel build ./music/index.html --public-url '/music' --no-source-maps --out-dir='dist/music'",
		"parcel-build": "yarn parcel-build-src && yarn parcel-build-music",
		"pre": "yarn time && yarn deduplicate && yarn screenshots && yarn insights && yarn fetch-music && yarn localga && yarn svg",
		"post": "yarn css && yarn tsc && yarn houdini && yarn parcel-build && yarn fetch-projects && yarn build-projects && yarn snap",
		"start": "yarn time && concurrently -r \"yarn css:watch\" \"yarn tsc:watch\" \"yarn houdini:watch\" \"yarn parcel-start\"",
		"start-music": "concurrently -r \"yarn houdini:watch\" \"yarn parcel-start-music\"",
		"start-dist": "http-server ./dist -p=1234 -P=http://localhost:1234/? -o",
		"build": "yarn pre && yarn post",
		"build-music": "concurrently -r \"yarn houdini\" \"yarn parcel-build-music\"",
		"postbuild": "workbox generateSW",
		"test": "jest --detectOpenHandles",
		"test:watch": "yarn test --watch",
		"test:coverage": "yarn test --coverage",
		"test:coverage:watch": "yarn test --coverage --watch",
		"lint": "./bin/lint.sh",
		"prettier": "./bin/prettier.sh",
		"deduplicate": "yarn-deduplicate --packages @types/react yarn.lock",
		"screenshots": "ts-node ./screenshots/index.ts",
		"insights": "ts-node ./insights/index.ts",
		"pages": "ts-node ./insights/pages.ts",
		"prerender": "ts-node ./server/index.ts",
		"pwa": "create-pwa --icon static/images/icons/icon.png --launch static/images/icons/splash.png",
		"type-coverage": "type-coverage --ignore-catch --detail",
		"please": "rm -rf .cache dist coverage node_modules yarn.lock",
		"snap": "react-snap"
	},
	"keywords": [],
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"repository": "https://github.com/scriptex/atanas.info",
	"bugs": {
		"url": "https://github.com/scriptex/atanas.info/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"license": "MIT",
