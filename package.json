{
	"name": "atanas.info",
	"version": "1.1.0",
	"description": "Atanas Atanasov's Personal Website",
	"scripts": {
		"fetch-music": "ts-node ./src/scripts/fetch-music.ts",
		"fetch-projects": "ts-node ./projects/.vuepress/fetch-projects.ts",
		"start-projects": "cd projects && vuepress dev",
		"build-projects": "cd projects && vuepress build",
		"localga": "localga --id UA-83446952-1 --folder ./src/scripts",
		"svgo": "svgo -f static/images/svg --config=svgo.yml",
		"svg-sprite": "spritesh -q -i static/images/svg -o static/sprite.svg -p svg-",
		"svg-minify": "html-minifier static/sprite.svg --collapse-whitespace -o static/sprite.svg",
		"svg": "yarn svgo && yarn svg-sprite && yarn svg-minify",
		"tsc": "tsc --noEmit",
		"tsc:watch": "tsc --noEmit --watch",
		"css": "postcss ./src/index.pcss -o ./src/index.css",
		"css:watch": "postcss ./src/index.pcss -o ./src/index.css -w",
		"parcel-start": "parcel ./src/index.html --open",
		"parcel-build-src": "rm -rf dist && parcel build ./src/index.html --public-url '/' --no-source-maps",
		"parcel-start-music": "parcel ./music/index.html --open --out-dir='dist/music'",
		"parcel-build-music": "parcel build ./music/index.html --public-url '/music' --no-source-maps --out-dir='dist/music'",
		"parcel-build": "yarn parcel-build-src && yarn parcel-build-music",
		"pre": "yarn deduplicate && yarn screenshots && yarn insights && yarn fetch-music && yarn localga && yarn svg",
		"start": "concurrently -r \"yarn css:watch\" \"yarn tsc:watch\" \"yarn parcel-start\"",
		"build": "yarn pre && yarn css && yarn tsc && yarn parcel-build && yarn fetch-projects && yarn build-projects",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "npm run lint:ts && npm run lint:css",
		"lint:ts": "eslint 'src/**/*.{ts,tsx}'",
		"lint:css": "stylelint './src/**/*.pcss' --config .stylelintrc",
		"deduplicate": "yarn-deduplicate --packages @types/react yarn.lock",
		"screenshots": "ts-node ./screenshots/index.ts",
		"insights": "ts-node ./insights/index.ts",
		"ssr": "ts-node ./server/index.ts",
		"pwa": "create-pwa --icon static/images/icons/icon.png --launch static/images/icons/splash.png",
		"type-coverage": "type-coverage --ignore-catch --detail"
	},
	"keywords": [],
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"repository": "https://github.com/scriptex/atanas.info",
	"bugs": {
		"url": "https://github.com/scriptex/atanas.info/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"license": "MIT",
	"engines": {
		"node": ">= 12.16.0",
		"npm": ">= 6.14.0"
	},
	"sw-precache": {
		"staticFileGlobs": [
			"dist/images/**/*",
			"dist/config.xml",
			"dist/manifest.webmanifest",
			"dist/resume.pdf",
			"dist/sprite.svg",
			"dist/**/*.css",
			"dist/**/*.js"
		],
		"stripPrefixMulti": {
			"dist/": "/"
		}
	},
	"dependencies": {
		"d3": "5.16.0",
		"date-fns": "2.15.0",
		"gsap": "3.4.0",
		"itcss": "1.5.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-helmet": "6.1.0",
		"react-inlinesvg": "2.0.0",
		"react-jinke-music-player": "4.16.3",
		"react-use": "15.3.2",
		"simplex-noise": "2.4.0",
		"universal-github-client": "1.0.3"
	},
	"devDependencies": {
		"@hot-loader/react-dom": "16.13.0",
		"@types/compression": "1.7.0",
		"@types/d3": "5.7.2",
		"@types/enzyme": "3.10.5",
		"@types/express": "4.17.7",
		"@types/jest": "26.0.4",
		"@types/node": "14.0.23",
		"@types/node-fetch": "2.5.7",
		"@types/puppeteer": "3.0.1",
		"@types/react": "16.9.43",
		"@types/react-dom": "16.9.8",
		"@types/react-helmet": "6.0.0",
		"@types/react-router-dom": "5.1.5",
		"@typescript-eslint/eslint-plugin": "3.6.1",
		"@typescript-eslint/parser": "3.6.1",
		"autoprefixer": "9.8.5",
		"cheerio": "1.0.0-rc.3",
		"cloudinary": "1.22.0",
		"compression": "1.7.4",
		"concurrently": "5.2.0",
		"create-pwa": "2.3.0",
		"cssnano": "4.1.10",
		"dotenv": "8.2.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.2",
		"enzyme-to-json": "3.5.0",
		"eslint": "7.5.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-jest": "23.18.0",
		"eslint-plugin-react": "7.20.3",
		"express": "4.17.1",
		"html-minifier": "4.0.0",
		"husky": "4.2.5",
		"jest": "26.1.0",
		"localga": "2.0.0",
		"node-fetch": "2.6.0",
		"parcel-bundler": "1.12.4",
		"parcel-plugin-static-files-copy": "2.4.3",
		"parcel-plugin-sw-precache": "1.1.2",
		"parcel-plugin-typescript-paths": "0.1.14",
		"postcss-cli": "7.1.1",
		"postcss-custom-media": "7.0.8",
		"postcss-flexbugs-fixes": "4.2.1",
		"postcss-for": "2.1.1",
		"postcss-import": "12.0.1",
		"postcss-merge-rules": "4.0.3",
		"postcss-mixins": "6.2.3",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.0.5",
		"puppeteer": "5.2.0",
		"react-imported-component": "6.3.5",
		"react-router-dom": "5.2.0",
		"sass": "1.26.10",
		"spritesh": "1.2.1",
		"stylelint": "13.6.1",
		"stylelint-config-recommended": "3.0.0",
		"svgo": "1.3.2",
		"ts-jest": "26.1.3",
		"ts-node": "8.10.2",
		"tslib": "2.0.0",
		"type-coverage": "2.9.0",
		"typescript": "3.9.7",
		"vuepress": "1.5.2",
		"yarn-deduplicate": "2.1.1"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"main": "src/index.tsx",
	"private": true,
	"resolutions": {
		"@types/react": "16.9.43"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn lint && yarn test"
		}
	},
	"typeCoverage": {
		"atLeast": 90
	}
}
