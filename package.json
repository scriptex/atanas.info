{
	"name": "atanas.info",
	"version": "1.1.0",
	"description": "Atanas Atanasov's Personal Website",
	"scripts": {
		"localga": "localga --id UA-83446952-1 --folder ./assets/scripts",
		"build:modern": "webpack --NODE_ENV=production --IS_MODERN",
		"build:legacy": "webpack --NODE_ENV=production",
		"start": "yarn localga && webpack --watch --NODE_ENV=development",
		"optisize": "optisize --src=\"./assets/images\"",
		"html": "php index.php > index.html",
		"rm-html": "rm index.html",
		"critical": "critical index.html -w 1280 -h 800 --ignore @font-face > assets/dist/critical.css",
		"prod": "yarn localga && yarn build:legacy && yarn build:modern && yarn html && yarn critical && yarn rm-html"
	},
	"keywords": [
		"MPA",
		"webpack",
		"setup",
		"multi-page"
	],
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"repository": "https://github.com/scriptex/webpack-mpa",
	"bugs": {
		"url": "https://github.com/scriptex/webpack-mpa/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"license": "MIT",
	"dependencies": {
		"@three11/animate-top-offset": "0.7.0",
		"d3": "5.14.2",
		"normalize.css": "8.0.1"
	},
	"devDependencies": {
