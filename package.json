		"@types/node": "14.14.20",
		"@types/node-fetch": "2.5.7",
		"@types/puppeteer": "5.4.2",
		"@types/react": "17.0.0",
		"@types/react-dom": "17.0.0",
		"@types/react-helmet": "6.1.0",
		"@types/react-slick": "0.23.4",
		"@types/react-vertical-timeline-component": "3.0.0",
		"@types/w3c-css-typed-object-model-level-1": "20180410.0.2",
		"@typescript-eslint/eslint-plugin": "4.13.0",
		"@typescript-eslint/parser": "4.13.0",
		"@vuepress/plugin-google-analytics": "1.8.0",
		"autoprefixer": "9.8.6",
		"cheerio": "1.0.0-rc.5",
		"cloudinary": "1.23.0",
		"concurrently": "5.3.0",
		"create-pwa": "2.3.0",
		"cssnano": "4.1.10",
		"dotenv": "8.2.0",
		"enzyme": "3.11.0",
		"enzyme-adapter-react-16": "1.15.5",
		"enzyme-to-json": "3.6.1",
		"eslint": "7.17.0",
		"eslint-config-prettier": "7.1.0",
		"eslint-plugin-jest": "24.1.3",
		"eslint-plugin-react": "7.22.0",
		"html-minifier": "4.0.0",
		"husky": "4.3.7",
		"jest": "26.6.3",
		"js-yaml": "4.0.0",
		"localga": "2.2.0",
		"node-fetch": "2.6.1",
		"parcel-bundler": "1.12.4",
		"parcel-plugin-static-files-copy": "2.5.0",
		"parcel-plugin-sw-precache": "1.1.2",
		"parcel-plugin-typescript-paths": "0.1.14",
		"postcss": "8.2.4",
		"postcss-calc": "8.0.0",
		"postcss-cli": "8.3.1",
		"postcss-custom-media": "7.0.8",
		"postcss-each": "0.10.0",
		"postcss-each-variables": "0.3.0",
		"postcss-easy-import": "3.0.0",
		"postcss-extend": "1.0.5",
		"postcss-flexbugs-fixes": "4.2.1",
		"postcss-for": "2.1.1",
		"postcss-import": "14.0.0",
		"postcss-merge-rules": "4.0.3",
		"postcss-mixins": "6.2.3",
		"postcss-nested": "5.0.3",
		"postcss-normalize": "9.0.0",
		"postcss-preset-env": "6.7.0",
		"postcss-utilities": "0.8.4",
		"prettier": "2.2.1",
		"puppeteer": "5.5.0",
		"sass": "1.32.2",
		"spritesh": "1.2.1",
		"stylelint": "13.8.0",
		"stylelint-config-recommended": "3.0.0",
		"svgo": "1.3.2",
		"svgo-viewbox": "1.0.1",
		"ts-jest": "26.4.4",
		"ts-node": "9.1.1",
		"tslib": "2.1.0",
		"type-coverage": "2.14.8",
		"typescript": "3.9.7",
		"vuepress": "1.8.0",
		"workbox-cli": "6.0.2",
		"yarn-deduplicate": "3.1.0"
	},
	"resolutions": {
		"@types/react": "17.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	],
	"main": "dist/index.html",
	"private": true,
	"husky": {
		"hooks": {
			"pre-push": "yarn lint && yarn prettier && yarn test"
		}
	},
	"typeCoverage": {
		"atLeast": 75
	}
}
