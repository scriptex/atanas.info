{
	"name": "atanas.info",
	"version": "1.1.0",
	"description": "Atanas Atanasov's Personal Website",
	"scripts": {
		"fetch-stats": "ts-node ./bin/fetch-stats.ts",
		"fetch-music": "ts-node ./bin/fetch-music.ts",
		"fetch-projects": "ts-node ./bin/fetch-projects.ts",
		"start-projects": "cd projects && vuepress dev",
		"build-projects": "cd projects && vuepress build",
		"localga": "ts-node ./bin/local-analytics.ts",
		"svg": "./bin/svg.sh",
		"tsc": "tsc --noEmit --skipLibCheck",
		"tsc:watch": "yarn tsc --watch",
		"houdini": "tsc -p tsconfig-houdini.json --skipLibCheck",
		"houdini:watch": "yarn houdini --watch",
		"time": "./bin/time.sh",
		"css": "postcss ./src/index.pcss -o ./src/index.css --config config",
		"css:watch": "postcss ./src/index.pcss -o ./src/index.css -w --config config",
		"parcel-start": "parcel ./src/index.html --open",
		"parcel-build": "rm -rf dist && parcel build ./src/index.html --public-url '/' --no-source-maps",
		"pre": "yarn time && yarn deduplicate && yarn screenshots && yarn insights && yarn npm-stats && yarn fetch-music && yarn localga && yarn svg",
		"post": "yarn css && yarn tsc && yarn houdini && yarn parcel-build && yarn fetch-projects && yarn build-projects",
		"start": "yarn time && concurrently -r \"yarn css:watch\" \"yarn tsc:watch\" \"yarn houdini:watch\" \"yarn parcel-start\"",
		"start-dist": "http-server ./dist -p=1234 -P=http://localhost:1234/? -o",
		"build": "yarn pre && yarn post",
		"postbuild": "workbox generateSW ./config/workbox-config.ts",
		"test": "jest --runInBand --config=./config/jest.config.ts --rootDir=./ --env=jsdom",
		"test:watch": "yarn test --watch",
		"test:coverage": "yarn test --coverage",
		"test:coverage:watch": "yarn test --coverage --watchAll",
		"lint": "./bin/lint.sh",
		"prettier": "./bin/prettier.sh",
		"deduplicate": "yarn-deduplicate --packages @types/react yarn.lock",
		"screenshots": "ts-node ./screenshots/index.ts",
		"insights": "ts-node ./insights/index.ts",
		"pages": "ts-node ./insights/pages.ts",
		"npm-stats": "ts-node ./npm/index.ts",
		"pwa": "create-pwa --icon static/images/icons/icon.png --launch static/images/icons/splash.png",
		"type-coverage": "type-coverage --ignore-catch --detail",
		"please": "rm -rf .cache dist coverage node_modules yarn.lock",
		"prerender": "react-spa-prerender",
		"postprerender": "ts-node server/index.ts"
	},
	"keywords": [],
