{
	"extends": [
		"stylelint-config-recommended"
	],
	"plugins": [
		"stylelint-no-unsupported-browser-features"
	],
	"rules": {
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"for",
					"mixin",
					"custom-media",
					"define-mixin"
				]
			}
		],
		"no-descending-specificity": null
	}
}
